<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="infnine.data">
<head>
</head>

<body>
<div metal:fill-slot="main">

    <div tal:replace="structure provider:plone.abovecontenttitle" />

    <h1 class="documentFirstHeading" tal:replace="context/title" />

    <div class="documentDescription" tal:content="context/description" />

    <img src="logo">

    <h2>Team</h2>
    <div tal:content="context/team">Team</div>

    <h2>Project Details</h2>
    <div tal:content="structure context/project_details">Project Details</div>

    <h2>Publications</h2>
    <div tal:content="context/publications">Publications</div>

    <h2>Media</h2>
    <!-- listing for media goes here -->

    <div tal:replace="structure provider:plone.abovecontentbody" />

    <div metal:use-macro="context/folder_listing/macros/listing" />

    <div tal:replace="structure provider:plone.belowcontentbody" />

</div>
</body>
</html>
