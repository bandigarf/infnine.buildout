<?xml version="1.0"?>
<object>
    <!-- Header - contained within plone.portaltop -->
    <order manager="plone.portalheader" skinname="Infnine Theme">
        <viewlet name="infnine.headerpic" />
        <viewlet name="infnine.logo" />
        <viewlet name="infnine.header1" />
    </order>
    
    <hidden manager="plone.portalheader" skinname="Infnine Theme">
        <viewlet name="plone.logo" />
        <viewlet name="plone.skip_links" />
        <viewlet name="plone.searchbox" />
        <viewlet name="plone.global_sections" />
        <viewlet name="plone.site_actions" />
    </hidden>
    
    <!-- portaltop -->
    <hidden manager="plone.portaltop" skinname="Infnine Theme">
        <viewlet name="plone.personal_bar" />
        <viewlet name="plone.app.i18n.locales.languageselector" />
        <viewlet name="plone.path_bar" />
    </hidden>
    
    <!-- footer -->
    <order manager="plone.portalfooter" skinname="Infnine Theme">
        <viewlet name="infnine.footer1" />
        <viewlet name="infnine.footer" />
        <viewlet name="plone.analytics" />
    </order>
    
    <hidden manager="plone.portalfooter" skinname="Infnine Theme">
        <viewlet name="plone.footer" />
        <viewlet name="plone.colophon" />
    </hidden>

</object>
