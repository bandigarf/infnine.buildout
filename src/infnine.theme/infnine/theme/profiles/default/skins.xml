<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Infnine Theme">

 <object name="infnine_theme_custom_images"
    meta_type="Filesystem Directory View"
    directory="infnine.theme:skins/infnine_theme_custom_images"/>
 <object name="infnine_theme_custom_templates"
    meta_type="Filesystem Directory View"
    directory="infnine.theme:skins/infnine_theme_custom_templates"/>
 <object name="infnine_theme_styles"
    meta_type="Filesystem Directory View"
    directory="infnine.theme:skins/infnine_theme_styles"/>

 <skin-path name="Infnine Theme" based-on="Plone Default">
  <layer name="infnine_theme_custom_images"
     insert-after="custom"/>
  <layer name="infnine_theme_custom_templates"
     insert-after="infnine_theme_custom_images"/>
  <layer name="infnine_theme_styles"
     insert-after="infnine_theme_custom_templates"/>
 </skin-path>

</object>
